{
  "name": "rs4",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "prestart:dev & nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "cli:dev": "ts-node -r tsconfig-paths/register src/cli.ts",
    "cli:prod": "node dist/src/cli.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "typeorm": "typeorm-ts-node-commonjs -d ./ormconfig.ts",
    "migration:generate": "ts-node node_modules/.bin/typeorm migration:generate -d ormconfig.ts migrations/$npm_config_name",
    "migration:run": "ts-node node_modules/.bin/typeorm migration:run -d ormconfig.ts",
    "migration:revert": "ts-node node_modules/.bin/typeorm migration:revert -d ormconfig.ts",
    "doc:serve": "npx compodoc -p tsconfig.json -w -s -r 8001",
    "doc:generate": "npx compodoc -p tsconfig.json",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "prestart:dev": "patch-package",
    "migrate": "npx prisma migrate dev",
    "start:migrate:prod": "npx prisma generate && prisma migrate dev && npx prisma db seed && npm run start:dev"
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed/seed.ts"
  },
  "dependencies": {
    "@casl/ability": "^6.3.3",
    "@casl/prisma": "^1.3.3",
    "@hapi/joi": "^17.1.1",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^9.1.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.1",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.1.1",
    "@nestjs/schedule": "^3.0.0",
    "@nestjs/swagger": "^6.1.2",
    "@prisma/client": "^4.10.1",
    "@selderee/plugin-htmlparser2": "^0.11.0",
    "@types/exceljs": "^1.3.0",
    "@types/express-validator": "^3.0.0",
    "@types/passport-http-bearer": "^1.0.37",
    "@types/uuid": "^8.3.4",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.2",
    "dotenv-expand": "^10.0.0",
    "exceljs": "^4.4.0",
    "handlebars": "^4.7.7",
    "helmet": "^7.0.0",
    "ip": "^1.1.8",
    "nestjs-cls": "^3.3.1",
    "nestjs-i18n": "^10.3.6",
    "nodemailer": "^6.9.1",
    "otplib": "^12.0.1",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.1.3",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.18",
    "@types/nodemailer": "^6.4.8",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vegardit/prisma-generator-nestjs-dto": "^1.5.1",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "glob": "^10.3.10",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^13.0.3",
    "patch-package": "^8.0.0",
    "prettier": "^2.7.1",
    "prisma": "^4.10.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.1",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      ".module.ts$"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
